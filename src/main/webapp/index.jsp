    <div class="mbr-cards-row row">
        <%int count=1;
        	ArrayList<ItemModel> itemDb=(ArrayList<ItemModel>)session.getAttribute("itemDb");
        	if(itemDb!=null)
        	{
        		for(ItemModel show: itemDb)
        		{
        			count++;
        			
        			if(count==12)
        			{
        				break;
        			}
